* Measure CNR_GR04_DIFF-AMP_PK
.control

*load {cicname}.raw

echo "MEAS_START"

*meas ac gm_db find vdb(out) when vp(out)=0
*meas ac pm_deg find phase when vdb(out)=0
*meas ac 3db_f when phase=135
*meas ac 0db_f when vdb(out)=0
*meas ac dc_gain find vdb(out) at=1

foreach vtemp -40 -20 0 20 40 80 125
    load {cicname}_$vtemp
    echo {cicname}_$vtemp
    meas ac gain_dc_$vtemp find vdb(out) at=1
end

echo "MEAS_END"
.endc
